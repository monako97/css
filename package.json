    "@emotion/serialize": "1.1.3",
    "@emotion/sheet": "1.2.2",
    "stylis": "4.3.0"
  },
  "devDependencies": {
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.101",
    "@types/stylis": "4.2.5",
    "typescript": "5.3.3"
  },
  "files": [
    "babel.d.ts",
    "babel.js",
